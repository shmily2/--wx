@import "../../common/vendor.wxss";

.weui-cell__bd {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  border-bottom: 2rpx solid #f1eded;
}
.weui-cell__td {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx;
}
.weui-cell__bd ._text {
  width: 30%;
}
.weui-input {
  text-align: right;
  width: 70%;
  font-size: 30rpx;
  color: rgb(94, 92, 92);
}
.rich {
  margin: 20rpx 30rpx;
}
.rich ._textarea {
  width: calc(100% - 20rpx);
  margin-top: 20rpx;
  border: 2rpx solid #f1eded;
  border-radius: 10rpx;
  padding: 10rpx;
}
.actiondata {
  padding: 20rpx;
  display: block;
}
.actiondata:not(:last-child) {
  border: 2rpx solid #f1eded;
}
.phcolor {
  font-size: 28rpx;
  color: #aab2bd;
  text-align: right;
}